@charset "UTF-8";
/* CSS Document */

#logo #nav-home input,
#header #top input {
	background:#E7E4D7 url(../images/bgSearch.gif) no-repeat scroll 2px 50%;
	border:1px solid #5C412F;
	padding:2px 3px 2px 20px;
	width: 115px;
	margin-left: 5px;
	font-family: Arial, Helvetica, sans-serif;
	color: #35261B;
	font-size: 10px;
}

#logo #nav-home select,
#header #top select {
	border:1px solid #5C412F;
	background: #E7E4D7;
	color: #35261B;
	padding: 3px;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 10px;
	width: 135px;
}
#mainContent p {
	line-height:1.3em;
}
#browsetype .pagination {
	margin-top: -20px;
}
#set.people li {
	margin-left: -39px;
}
